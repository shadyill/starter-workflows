  devsite-snackbar [type="cookie-notification"] .devsite-snackbar-message, devsite-snackbar [type^="notification-"] .devsite-snackbar-message { -webkit-box-flex: 0; flex: 0 0 100%; padding-bottom: 0px; }
  devsite-snackbar .devsite-snackbar-inner { border-radius: 0px; }
  devsite-snackbar .devsite-snackbar-snack { left: 0px; padding: 24px 0px 0px; right: 0px; }
}
